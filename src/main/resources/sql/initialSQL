drop table LARGEAMOUNT_INVESTOR;
drop table INSTITUTIONAL_INVESTOR;
drop table FUTURES_DAILY;

CREATE TABLE `options`.`LARGEAMOUNT_INVESTOR` (
  `Id` INT NOT NULL AUTO_INCREMENT,
  `DATE` DATE NULL COMMENT '日期',
  `CONTRACT_NAME` VARCHAR(50) NULL COMMENT '契約名稱',
  `PERIOD` VARCHAR(20) NULL COMMENT '到期月份(週別)',
  `TOP5_HOLDER_LOT` INT NULL COMMENT '前五買方部位數',
  `SPECIAL_TOP5_HOLDER_LOT` INT NULL COMMENT '特定法人前五買方部位數',
  `TOP5_HOLDER_PERCENT` FLOAT NULL COMMENT '前五買方百分比',
  `SPECIAL_TOP5_HOLDER_PERCENT` FLOAT NULL COMMENT '特定法人前五買方百分比',
  `TOP10_HOLDER_LOT` INT NULL COMMENT '前十買方部位數',
  `SPECIAL_TOP10_HOLDER_LOT` INT NULL COMMENT '特定法人前十買方部位數',
  `TOP10_HOLDER_PERCENT` FLOAT NULL COMMENT '前十買方百分比',
  `SPECIAL_TOP10_HOLDER_PERCENT` FLOAT NULL COMMENT '特定法人前十買方百分比',
  `TOP5_SELLER_LOT` INT NULL COMMENT '前五賣方部位數',
  `SPECIAL_TOP5_SELLER_LOT` INT NULL COMMENT '特定法人前五賣方部位數',
  `TOP5_SELLER_PERCENT` FLOAT NULL COMMENT '前五賣方百分比',
  `SPECIAL_TOP5_SELLER_PERCENT` FLOAT NULL COMMENT '特定法人前五賣方百分比',
  `TOP10_SELLER_LOT` INT NULL COMMENT '前十賣方部位數',
  `SPECIAL_TOP10_SELLER_LOT` INT NULL COMMENT '特定法人前十賣方部位數',
  `TOP10_SELLER_PERCENT` FLOAT NULL COMMENT '前十賣方百分比',
  `SPECIAL_TOP10_SELLER_PERCENT` FLOAT NULL COMMENT '特定法人前十賣方百分比',
  `BALANCE_KEEP_LOT` INT NULL COMMENT '全市場未沖銷部位數',
  PRIMARY KEY (`Id`));

  CREATE TABLE `options`.`INSTITUTIONAL_INVESTOR` (
  `id` INT NOT NULL AUTO_INCREMENT,
  `DATE` DATE NULL COMMENT '日期',
  `OPTION_TYPE` VARCHAR(10) NULL COMMENT '權別',
  `INVESTOR` VARCHAR(10) NULL COMMENT '身分別',
  `HOLDER_DEALT_LOT` INT NULL COMMENT '買方口數',
  `HOLDER_DEALT_AMOUNT` INT NULL COMMENT '買方契約金額',
  `SELLER_DEALT_LOT` INT NULL COMMENT '賣方口數',
  `SELLER_DEALT_AMOUNT` INT NULL COMMENT '賣方契約金額',
  `BALANCE_DEALT_LOT` INT NULL COMMENT '口數差額',
  `BALANCE_DEALT_AMOUNT` INT NULL COMMENT '契約金差額',
  `HOLDER_KEEP_LOT` INT NULL COMMENT '未平倉買方口數',
  `HOLDER_KEEP_AMOUNT` INT NULL COMMENT '未平倉買方契約金額',
  `SELLER_KEEP_LOT` INT NULL COMMENT '未平倉賣方口數',
  `SELLER_KEEP_AMOUNT` INT NULL COMMENT '未平倉賣方契約金額',
  `BALANCE_KEEP_LOT` INT NULL COMMENT '未平倉口數差額',
  `BALANCE_KEEP_AMOUNT` INT NULL COMMENT '未平倉契約金差額',
  PRIMARY KEY (`id`));
  
CREATE TABLE `options`.`FUTURES_DAILY` (
`id` INT NOT NULL AUTO_INCREMENT,
`DATE` DATE NULL COMMENT '日期',
`CONTRACT` VARCHAR(10) NULL COMMENT '契約',
`PERIOD` VARCHAR(10) NULL COMMENT '到期月份(週別)',
`OPENING_PRICE` INT NULL COMMENT '開盤價',
`HIGHEST_PRICE` INT NULL COMMENT '最高價',
`LOWEST_PRICE` INT NULL COMMENT '最低價',
`FINAL_PRICE` INT NULL COMMENT '最後成交價',
`CHANGE` INT NULL COMMENT '漲跌價',
`CHANGE_PERCENT` INT NULL COMMENT '漲跌%',
`AFTER_TRADE_TIME_AMOUNT` INT NULL COMMENT '盤後交易時段成交量',
`NORMAL_TRADE_TIME_AMOUNT` INT NULL COMMENT '一般交易時段成交量',
`TOTAL_AMOUNT` INT NULL COMMENT '合計成交量',
`SETTLEMENT_PRICE` INT NULL COMMENT '結算價',
`OPEN_INTEREST` INT NULL COMMENT '未沖銷契約量',
`FINAL_BUY_PRICE` INT NULL COMMENT '最後最佳買價',
`FINAL_SELL_PRICE` INT NULL COMMENT '最後最佳賣價',
`HIGHEST_IN_HISTORY` INT NULL COMMENT '歷史最高價',
`LOWEST_IN_HISTORY` INT NULL COMMENT '歷史最低價',
PRIMARY KEY (`id`));